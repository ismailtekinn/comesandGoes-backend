{
  "name": "comesandgoes-api",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "build": "ts-node",
    "start": "nodemon --exec ts-node index.ts",
    "migrate": "npx prisma migrate dev",
    "lint": "eslint .",
    "format": "prettier --write .",
    "forever-start": "forever start -a -l forever.log -o out.log -e err.log -c ts-node --transpile-only daemon index.ts",
    "forever-stop": "forever stop index.ts",
    "forever-restart": "forever restart index.ts",
    "forever-list": "forever list"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.4",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "forever": "^4.0.3",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "prisma": "^5.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
}
